    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "winston": "^2.4.1",
    "winston-daily-rotate-file": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-prettier-plugin": "^1.2.0",
