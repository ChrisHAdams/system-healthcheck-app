  "version": "1.0.0",
  "description": "Healthcheck app for monitoring services, databases and APIs.",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/nodemon/bin/nodemon.js index.js",
    "prettier": "gulp prettier",
    "watch": "./node_modules/nodemon/bin/nodemon.js index.js",
