
{

  "env" : {
      "browser" : true,
      "node" : true,
      "es6" : true
  },
  "rules" : {
      "no-unused-vars" : 2,
      "no-undef" : 2
  },
  "parserOptions": {

      "jsx": true,
      "sourceType": "module",
      "modules": true,
      "spread" : true,
      "restParams" : true

  }
}